{
    "name": "nahid/jsonq",
    "description": "JsonQ is a PHP based json data query library",
    "keywords": ["json", "jsonq", "query"],
    "homepage": "https://github.com/nahid/jsonq",
    "type": "library",
    "license": "CC-BY-3.0",
    "authors": [
        {
            "name": "Nahid Bin Azhar",
            "email": "nahid.dns@gmail.com"
        }
    ],
  "require": {
    "php": ">=5.5.0"
  },
  "require-dev": {
        "phpunit/phpunit": "^4.8 || ^5.0",
        "symfony/var-dumper": "^2.7",
        "graham-campbell/testbench": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Nahid\\JsonQ\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Nahid\\JsonQ\\Tests\\": "tests/"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
